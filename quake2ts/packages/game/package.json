{
  "name": "@quake2ts/game",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.js"
    },
    "./browser": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/browser/index.js"
    }
  },
  "unpkg": "./dist/browser/index.js",
  "jsdelivr": "./dist/browser/index.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run build:types && pnpm run build:bundles",
    "build:types": "tsc -b",
    "build:bundles": "tsup",
    "test": "pnpm run test:unit",
    "test:unit": "TEST_TYPE=unit vitest run --passWithNoTests",
    "test:integration": "TEST_TYPE=integration vitest run --poolOptions.forks.maxForks=1 tests/integration"
  },
  "dependencies": {
    "@quake2ts/engine": "workspace:*",
    "@quake2ts/shared": "workspace:*"
  },
  "devDependencies": {
    "@quake2ts/test-utils": "workspace:^",
    "@quake2ts/tests": "workspace:^",
    "fake-indexeddb": "^6.0.0",
    "tsup": "^8.1.0"
  }
}
