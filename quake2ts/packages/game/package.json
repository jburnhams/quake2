{
  "name": "@quake2ts/game",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.js"
    },
    "./browser": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/browser/index.js"
    }
  },
  "unpkg": "./dist/browser/index.js",
  "jsdelivr": "./dist/browser/index.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run build:types && pnpm run build:bundles",
    "build:types": "tsc -b",
    "build:bundles": "tsup",
    "test": "pnpm run test:unit",
    "test:unit": "TEST_TYPE=unit pnpm run test:unit:entities && TEST_TYPE=unit pnpm run test:unit:combat && TEST_TYPE=unit pnpm run test:unit:physics && TEST_TYPE=unit pnpm run test:unit:ai && TEST_TYPE=unit pnpm run test:unit:inventory && TEST_TYPE=unit pnpm run test:unit:core && TEST_TYPE=unit pnpm run test:unit:replay",
    "test:integration": "TEST_TYPE=integration vitest run tests/integration/entitySystem.test.ts && TEST_TYPE=integration vitest run tests/integration/combat.test.ts && TEST_TYPE=integration vitest run tests/integration/gameplay.test.ts",
    "test:unit:entities": "vitest run --passWithNoTests tests/entities",
    "test:unit:combat": "vitest run --passWithNoTests tests/combat",
    "test:unit:physics": "vitest run --passWithNoTests tests/physics",
    "test:unit:ai": "vitest run --passWithNoTests tests/ai",
    "test:unit:inventory": "vitest run --passWithNoTests tests/inventory",
    "test:unit:core": "vitest run --passWithNoTests tests/*.test.ts src/save/tests",
    "test:unit:replay": "vitest run --passWithNoTests tests/replay"
  },
  "dependencies": {
    "@quake2ts/engine": "workspace:*",
    "@quake2ts/shared": "workspace:*"
  },
  "devDependencies": {
    "@quake2ts/test-utils": "workspace:^",
    "@quake2ts/tests": "workspace:^",
    "fake-indexeddb": "^6.0.0",
    "tsup": "^8.1.0"
  }
}
