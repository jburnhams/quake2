{
  "name": "@quake2ts/game",
  "version": "0.0.825",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.js"
    },
    "./browser": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/browser/index.js"
    }
  },
  "unpkg": "./dist/browser/index.js",
  "jsdelivr": "./dist/browser/index.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jburnhams/quake2.git",
    "directory": "quake2ts/packages/game"
  },
  "scripts": {
    "build": "pnpm run build:types && pnpm run build:bundles",
    "build:types": "tsc -b",
    "build:bundles": "tsup",
    "test": "pnpm run test:unit",
    "test:unit": "TEST_TYPE=unit vitest run --passWithNoTests",
    "test:integration": "TEST_TYPE=integration NODE_OPTIONS='--max-old-space-size=4096' vitest run tests/integration/entitySystem.test.ts tests/integration/combat.test.ts tests/integration/gameplay.test.ts tests/integration/hooks.test.ts tests/integration/scenario_base1.test.ts tests/integration/physicsStress.test.ts tests/integration/saveLoad.test.ts tests/integration/asset-loading.test.ts"
  },
  "peerDependencies": {
    "@quake2ts/engine": "workspace:*",
    "@quake2ts/shared": "workspace:*",
    "gl-matrix": "^3.4.4"
  },
  "devDependencies": {
    "@quake2ts/engine": "workspace:*",
    "@quake2ts/shared": "workspace:*",
    "@quake2ts/test-utils": "workspace:^",
    "fake-indexeddb": "^6.2.5",
    "gl-matrix": "^3.4.4",
    "tsup": "^8.5.1"
  }
}
