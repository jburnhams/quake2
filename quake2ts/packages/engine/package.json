{
  "name": "@quake2ts/engine",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.js"
    },
    "./browser": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/browser/index.js"
    }
  },
  "unpkg": "./dist/browser/index.js",
  "jsdelivr": "./dist/browser/index.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run build:types && pnpm run build:bundles",
    "build:types": "tsc -b",
    "build:bundles": "tsup",
    "test": "vitest run --passWithNoTests"
  },
  "dependencies": {
    "@quake2ts/shared": "workspace:*",
    "@wasm-audio-decoders/ogg-vorbis": "^0.1.20",
    "gl-matrix": "^3.4.3"
  },
  "devDependencies": {
    "@quake2ts/game": "workspace:*",
    "fake-indexeddb": "^6.0.0",
    "jsdom": "^27.2.0",
    "tsup": "^8.1.0",
    "vitest": "^1.5.3"
  }
}
