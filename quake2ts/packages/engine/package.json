{
  "name": "@quake2ts/engine",
  "version": "0.0.797",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.js"
    },
    "./browser": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/browser/index.js"
    }
  },
  "unpkg": "./dist/browser/index.js",
  "jsdelivr": "./dist/browser/index.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jburnhams/quake2.git",
    "directory": "quake2ts/packages/engine"
  },
  "scripts": {
    "build": "pnpm run build:types && pnpm run build:bundles",
    "build:types": "tsc -b",
    "build:bundles": "tsup",
    "test": "pnpm run test:unit",
    "test:unit": "TEST_TYPE=unit vitest run --passWithNoTests",
    "test:integration": "TEST_TYPE=integration vitest run --passWithNoTests",
    "test:webgpu": "cross-env TEST_TYPE=webgpu vitest run --passWithNoTests",
    "test:webgl": "cross-env TEST_TYPE=webgl vitest run --passWithNoTests",
    "test:webgl:watch": "cross-env TEST_TYPE=webgl vitest watch",
    "test:webgl:update": "cross-env UPDATE_VISUAL=1 TEST_TYPE=webgl vitest run",
    "test:webgl:ui": "cross-env TEST_TYPE=webgl vitest --ui",
    "test:visual": "vitest run tests/integration/visual",
    "test:visual:update": "vitest run tests/integration/visual --update-snapshots",
    "test:visual:watch": "vitest tests/integration/visual"
  },
  "_comment_gl": "gl is optional only in test-utils and installed on demand and in the webgl github action",
  "peerDependencies": {
    "@quake2ts/shared": "workspace:*",
    "gl-matrix": "^3.4.4"
  },
  "dependencies": {
    "@wasm-audio-decoders/ogg-vorbis": "^0.1.20"
  },
  "devDependencies": {
    "@napi-rs/canvas": "^0.1.86",
    "@quake2ts/game": "workspace:*",
    "@quake2ts/shared": "workspace:*",
    "@quake2ts/test-utils": "workspace:^",
    "@types/node": "^25.0.3",
    "@webgpu/types": "^0.1.68",
    "fake-indexeddb": "^6.2.5",
    "gl-matrix": "^3.4.4",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0",
    "tsup": "^8.5.1",
    "vitest": "^4.0.16",
    "webgpu": "^0.3.8"
  }
}
