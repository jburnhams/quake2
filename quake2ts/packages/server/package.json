{
  "name": "@quake2ts/server",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "test": "vitest",
    "build": "pnpm run build:types && pnpm run build:bundles",
    "build:types": "tsc -b",
    "build:bundles": "tsup",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@quake2ts/shared": "workspace:*",
    "@quake2ts/engine": "workspace:*",
    "@quake2ts/game": "workspace:*",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.10",
    "vitest": "^1.2.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
