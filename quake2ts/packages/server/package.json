{
  "name": "@quake2ts/server",
  "version": "0.0.878",
  "description": "Quake II server implementation for Node.js.",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jburnhams/quake2.git",
    "directory": "quake2ts/packages/server"
  },
  "scripts": {
    "build": "pnpm run build:types && pnpm run build:bundles",
    "build:types": "tsc -b",
    "build:bundles": "tsup",
    "test": "pnpm run test:unit",
    "test:unit": "TEST_TYPE=unit vitest run",
    "test:unit-node": "TEST_TYPE=unit-node vitest run",
    "test:unit-jsdom": "TEST_TYPE=unit-jsdom vitest run",
    "test:integration": "TEST_TYPE=integration vitest run"
  },
  "dependencies": {
    "@quake2ts/engine": "workspace:*",
    "@quake2ts/game": "workspace:*",
    "@quake2ts/shared": "workspace:*",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@quake2ts/test-utils": "workspace:*",
    "@types/ws": "^8.5.10",
    "tsup": "^8.5.1"
  }
}
