{
  "name": "@quake2ts/server",
  "version": "0.0.812",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jburnhams/quake2.git",
    "directory": "quake2ts/packages/server"
  },
  "scripts": {
    "build": "pnpm run build:types && pnpm run build:bundles",
    "build:types": "tsc -b",
    "build:bundles": "tsup",
    "dev": "tsup --watch",
    "test": "pnpm run test:unit",
    "test:unit": "TEST_TYPE=unit vitest run",
    "test:integration": "TEST_TYPE=integration vitest run"
  },
  "peerDependencies": {
    "@quake2ts/engine": "workspace:*",
    "@quake2ts/game": "workspace:*",
    "@quake2ts/shared": "workspace:*"
  },
  "dependencies": {
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@quake2ts/engine": "workspace:*",
    "@quake2ts/game": "workspace:*",
    "@quake2ts/shared": "workspace:*",
    "@quake2ts/test-utils": "workspace:*",
    "@types/ws": "^8.18.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
