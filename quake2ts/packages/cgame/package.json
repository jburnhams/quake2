{
  "name": "@quake2ts/cgame",
  "version": "0.0.875",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.js"
    },
    "./browser": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/browser/index.js"
    }
  },
  "unpkg": "./dist/browser/index.js",
  "jsdelivr": "./dist/browser/index.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jburnhams/quake2.git",
    "directory": "quake2ts/packages/cgame"
  },
  "scripts": {
    "build": "pnpm run build:types && pnpm run build:bundles",
    "build:types": "tsc -b",
    "build:bundles": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "test": "vitest run --config vitest.node.ts",
    "test:unit:node": "vitest run --config vitest.node.ts",
    "test:unit:jsdom": "vitest run --config vitest.jsdom.ts",
    "test:integration": "vitest run --config vitest.integration.ts",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@quake2ts/engine": "workspace:*",
    "@quake2ts/game": "workspace:*",
    "@quake2ts/shared": "workspace:*",
    "gl-matrix": "^3.4.4"
  },
  "devDependencies": {
    "@quake2ts/engine": "workspace:*",
    "@quake2ts/game": "workspace:*",
    "@quake2ts/shared": "workspace:*",
    "@types/node": "^25.0.3",
    "gl-matrix": "^3.4.4",
    "jsdom": "27.4.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
