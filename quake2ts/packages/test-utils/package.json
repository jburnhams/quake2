{
  "name": "@quake2ts/test-utils",
  "version": "0.0.771",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./src/engine/mocks/webgpu": {
      "types": "./src/engine/mocks/webgpu.ts",
      "import": "./src/engine/mocks/webgpu.ts",
      "require": "./src/engine/mocks/webgpu.ts"
    },
    "./src/setup/webgpu": {
      "types": "./src/setup/webgpu.ts",
      "import": "./src/setup/webgpu.ts",
      "require": "./src/setup/webgpu.ts"
    },
    "./src/setup/headless-webgl": {
      "types": "./src/setup/headless-webgl.ts",
      "import": "./src/setup/headless-webgl.ts",
      "require": "./src/setup/headless-webgl.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jburnhams/quake2.git",
    "directory": "quake2ts/packages/test-utils"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "test": "vitest run --passWithNoTests",
    "test:webgpu": "cross-env TEST_TYPE=webgpu vitest run",
    "test:webgl": "cross-env TEST_TYPE=webgl vitest run"
  },
  "peerDependencies": {
    "@quake2ts/engine": "workspace:^",
    "@quake2ts/game": "workspace:*",
    "@quake2ts/server": "workspace:*",
    "@quake2ts/shared": "workspace:*",
    "@webgpu/types": "^0.1.68",
    "gl-matrix": "^3.4.4",
    "pixelmatch": "^7.1.0",
    "playwright": "^1.57.0",
    "pngjs": "^7.0.0",
    "vitest": "^1.6.0",
    "webgpu": "^0.3.8"
  },
