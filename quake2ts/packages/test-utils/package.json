{
  "name": "@quake2ts/test-utils",
  "version": "0.0.728",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./src/engine/mocks/webgpu": {
      "types": "./src/engine/mocks/webgpu.ts",
      "import": "./src/engine/mocks/webgpu.ts",
      "require": "./src/engine/mocks/webgpu.ts"
    },
    "./src/setup/webgpu": {
      "types": "./src/setup/webgpu.ts",
      "import": "./src/setup/webgpu.ts",
      "require": "./src/setup/webgpu.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "test": "vitest run --passWithNoTests",
    "test:webgpu": "cross-env TEST_TYPE=webgpu vitest run"
  },
  "dependencies": {
    "@quake2ts/engine": "workspace:^",
    "@quake2ts/game": "workspace:*",
    "@quake2ts/server": "workspace:*",
    "@quake2ts/shared": "workspace:*",
    "gl-matrix": "^3.4.4"
  },
  "peerDependencies": {
    "@webgpu/types": "^0.1.68",
    "pixelmatch": "^7.1.0",
    "playwright": "^1.57.0",
    "pngjs": "^7.0.0",
    "vitest": "^1.6.0",
    "webgpu": "^0.3.8"
  },
  "peerDependenciesMeta": {
    "@webgpu/types": {
      "optional": true
    },
    "pixelmatch": {
      "optional": true
    },
    "playwright": {
      "optional": true
    },
    "pngjs": {
      "optional": true
    },
    "webgpu": {
      "optional": true
    }
  },
  "devDependencies": {
    "@napi-rs/canvas": "^0.1.84",
    "@types/jsdom": "^27.0.0",
    "@types/pixelmatch": "^5.2.6",
    "@types/pngjs": "^6.0.5",
    "@webgpu/types": "^0.1.68",
    "fake-indexeddb": "^6.0.0",
    "jsdom": "^27.3.0",
    "pixelmatch": "^7.1.0",
    "playwright": "^1.57.0",
    "pngjs": "^7.0.0",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "webgpu": "^0.3.8"
  }
}
