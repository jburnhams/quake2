{
  "name": "@quake2ts/shared",
  "version": "0.0.801",
  "description": "Shared math, physics, and protocol helpers for the Quake II TypeScript port.",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.js"
    },
    "./browser": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/browser/index.js"
    },
    "./testing": "./src/testing.ts",
    "./bsp/collision": "./src/bsp/collision.ts",
    "./math/vec3": "./src/math/vec3.ts"
  },
  "unpkg": "./dist/browser/index.js",
  "jsdelivr": "./dist/browser/index.js",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jburnhams/quake2.git",
    "directory": "quake2ts/packages/shared"
  },
  "scripts": {
    "build": "pnpm run build:types && pnpm run build:bundles",
    "build:types": "tsc -b",
    "build:bundles": "tsup",
    "test": "pnpm run test:unit",
    "test:unit": "TEST_TYPE=unit vitest run",
    "test:integration": "TEST_TYPE=integration vitest run"
  },
  "devDependencies": {
    "gl-matrix": "^3.4.4",
    "tsup": "^8.5.1"
  }
}
