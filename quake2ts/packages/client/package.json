{
  "name": "@quake2ts/client",
  "version": "0.0.848",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.js"
    },
    "./browser": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/browser/index.js"
    }
  },
  "unpkg": "./dist/browser/index.js",
  "jsdelivr": "./dist/browser/index.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jburnhams/quake2.git",
    "directory": "quake2ts/packages/client"
  },
  "scripts": {
    "build": "pnpm run build:types && pnpm run build:bundles",
    "build:types": "tsc -b",
    "build:bundles": "tsup",
    "test": "pnpm run test:unit",
    "test:unit": "pnpm run test:unit:node && pnpm run test:unit:jsdom",
    "test:unit:node": "NODE_OPTIONS='--max-old-space-size=4096' TEST_TYPE=unit vitest run -c vitest.node.ts --passWithNoTests",
    "test:unit:jsdom": "NODE_OPTIONS='--max-old-space-size=4096' TEST_TYPE=unit vitest run -c vitest.jsdom.ts --passWithNoTests",
    "test:integration": "NODE_OPTIONS='--max-old-space-size=4096' TEST_TYPE=integration vitest run -c vitest.integration.ts --passWithNoTests"
  },
  "peerDependencies": {
    "@quake2ts/cgame": "workspace:^",
    "@quake2ts/engine": "workspace:*",
    "@quake2ts/game": "workspace:*",
    "@quake2ts/shared": "workspace:*",
    "gl-matrix": "^3.4.4"
  },
  "devDependencies": {
    "@quake2ts/cgame": "workspace:^",
    "@quake2ts/engine": "workspace:*",
    "@quake2ts/game": "workspace:*",
    "@quake2ts/shared": "workspace:*",
    "@quake2ts/test-utils": "workspace:^",
    "gl-matrix": "^3.4.4",
    "tsup": "^8.5.1"
  }
}
