{
  "name": "@quake2ts/client",
  "version": "0.0.1",
  "description": "Quake 2 Client Implementation",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "browser": "./dist/browser/index.global.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.js"
    },
    "./browser": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/browser/index.global.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run build:types && pnpm run build:bundles",
    "build:types": "tsc -b",
    "build:bundles": "tsup",
    "test": "vitest run"
  },
  "dependencies": {
    "@quake2ts/cgame": "workspace:*",
    "@quake2ts/engine": "workspace:*",
    "@quake2ts/game": "workspace:*",
    "@quake2ts/shared": "workspace:*",
    "gl-matrix": "^3.4.3"
  },
  "devDependencies": {
    "@quake2ts/test-utils": "workspace:*",
    "@types/node": "^20.11.24",
    "jsdom": "^24.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  }
}
